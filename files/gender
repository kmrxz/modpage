  <script>
        const rangeInput = document.getElementById('gender-range');
        const rangeValue = document.getElementById('range-value');
        
        // Update range label based on slider value
        rangeInput.addEventListener('input', function() {
            rangeValue.textContent = rangeInput.value == 0 ? 'Male' : 'Female';
        });
        function Submit(){
          document.getElementById('opt').innerHTML="You're either a Male or female";
          document.getElementById('opt2').innerHTML="Think about it";
        };
    </script> 
